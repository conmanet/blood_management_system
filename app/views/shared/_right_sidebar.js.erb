$(document).ready(function() {
  renderUpcommingEvents();
  renderNewArticles();
});

function renderUpcommingEvents() {
  var url = "<%= ajax_right_sidebar_events_path %>";
  $.get(url, {}, null, "script");
}

function renderNewArticles() {
  var url = "<%= ajax_right_sidebar_articles_path %>";
  $.get(url, {}, null, "script");
}
