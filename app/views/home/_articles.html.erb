<section id="courseArchive">
  <div class="container">
    <div class="row">
      <div class="col-lg-12 col-md-12">
        <div class="title_area">
          <h2 class="title_two">Bài viết mới</h2>
          <span></span>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-md-12">
        <div class="courseArchive_content">
          <%= render partial: "article", collection: @articles %>
          <div class="row">
            <div class="col-md-12">
              <%= link_to "Xem thêm", guest_articles_path, class: "see_all" %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
